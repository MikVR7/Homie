{
  "example_1_images_with_metadata": {
    "description": "Organizing photos with EXIF metadata",
    "request": {
      "files_with_metadata": [
        {
          "path": "/Users/john/Downloads/IMG_1234.jpg",
          "metadata": {
            "size": 2048000,
            "last_modified": "2025-01-15T10:30:00Z",
            "created": "2025-01-12T14:20:00Z",
            "extension": ".jpg",
            "image": {
              "width": 1920,
              "height": 1080,
              "format": "JPEG",
              "date_taken": "2025-01-12T14:20:00Z",
              "camera_model": "Canon EOS R5",
              "location": "Vienna, Austria"
            }
          }
        },
        {
          "path": "/Users/john/Downloads/IMG_1235.jpg",
          "metadata": {
            "size": 1856000,
            "extension": ".jpg",
            "image": {
              "width": 1920,
              "height": 1080,
              "date_taken": "2025-01-12T15:45:00Z",
              "camera_model": "Canon EOS R5",
              "location": "Vienna, Austria"
            }
          }
        }
      ],
      "source_path": "/Users/john/Downloads",
      "destination_path": "/Users/john/Pictures",
      "organization_style": "by_type",
      "user_id": "john_doe",
      "client_id": "johns_macbook"
    },
    "expected_result": "Both images organized to Pictures/Vienna2025/ based on location and date"
  },

  "example_2_project_archive": {
    "description": "Organizing a project archive with detected project type",
    "request": {
      "files_with_metadata": [
        {
          "path": "/Users/john/Downloads/MyReactApp.zip",
          "metadata": {
            "size": 5242880,
            "extension": ".zip",
            "archive": {
              "archive_type": "ZIP",
              "contents": [
                "package.json",
                "src/App.tsx",
                "src/index.tsx",
                "public/index.html",
                "README.md"
              ],
              "detected_project_type": "React",
              "contains_executables": false
            }
          }
        }
      ],
      "source_path": "/Users/john/Downloads",
      "destination_path": "/Users/john/Projects",
      "user_id": "john_doe",
      "client_id": "johns_macbook"
    },
    "expected_result": "Archive extracted to Projects/React/MyReactApp/"
  },

